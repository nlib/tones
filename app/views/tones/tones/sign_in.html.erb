<h2>Login to tones</h2>
<div class='tones_sign_in col-md-6'>
  <%= form_tag(sign_in_tones_path) do %>
    <div>
      <%= label_tag 'password', 'Enter the password' %>
      </br>
      <%= password_field_tag 'password', '', class: 'form-control' %>
    </div>
    </br>
    <div>
      <%= submit_tag 'Sign in', class: 'btn btn-primary' %>
    </div>
  <% end %>
</div>
